{
    "version": 2,
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "env": {
        "NEXT_PUBLIC_API_URL": "https://pravah-house-api.onrender.com"
    },
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "https://pravah-house-api.onrender.com/api/v1/$1"
        }
    ]
}